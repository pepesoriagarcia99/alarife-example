{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "sourceMaps": "inline",
      "retainLines": true
    }
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      // ** Proposals in Stage 3
      "@babel/plugin-proposal-decorators",
      {
        "version": "2023-05"
      }
    ]
  ]
}